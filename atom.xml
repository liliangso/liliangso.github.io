  <entry>
    <title>简单工厂</title>
    <link href="http://example.com/2022/01/05/%E7%AE%80%E5%8D%95%E5%B7%A5%E5%8E%82/"/>
    <id>http://example.com/2022/01/05/%E7%AE%80%E5%8D%95%E5%B7%A5%E5%8E%82/</id>
